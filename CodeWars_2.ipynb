   "source": [
    "def add_binary(a,b):\n",
    "    #your code here\n",
    "    c = a + b\n",
    "    string = ''\n",
    "    while (c != 0):\n",
    "        string += str(c%2)\n",
    "        c //= 2\n",
    "    return string[::-1]"
   ]
